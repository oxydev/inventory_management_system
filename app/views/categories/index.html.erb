<h1>Index</h1>
<%= current_user.user_name %>

<table>
    <tr class="header">
      <th> # </th>
      <th>Category Name</th>
      <th>No of Asset</th>
      <th>Actions</th>
    </tr>
    <% @categories.each do |category| %>
    <tr>
      <td><%= category.id %> </td>
      <td><%= category.category %></td>
      <td>1</td>
      <td>
        <%= link_to("Show", category_path(category)) %>
        <%= link_to("Edit", edit_category_path(category)) %>
        <%= link_to("Delete", "#") %>
      </td>
    </tr>
    <% end %>
  </table>
